{
    "parser": "babel-eslint",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier",
      "node"
    ],
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "rules": {
      "node/no-unsupported-features": "error",
      "no-unused-vars": "warn",
      "prettier/prettier": ["error", {
        "singleQuote": true,
        "semi": false
      }]
    },
    "env": {
      "es6": true,
      "node": true
    }
}
