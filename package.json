  "devDependencies": {
    "@babel/cli": "7.25.6",
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-typescript": "7.24.7",
    "@connectedcars/setup": "^1.1.3",
    "@connectedcars/test": "^2.1.0",
    "@eslint/js": "9.9.1",
    "@types/benchmark": "^2.1.5",
    "@types/express": "^5.0.1",
    "@types/jest": "29.5.12",
    "@types/node": "^22.15.3",
    "@types/sinon": "^17.0.4",
    "@types/tmp": "0.1.0",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "axios": "^1.9.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "29.7.0",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.6",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "express": "^4.17.1",
    "globals": "15.9.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "prettier": "3.3.3",
    "r2": "^2.0.1",
